{
  "name": "app-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "jest-junit": "^5.1.0",
    "raf": "^3.4.0"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
